  },
  "main": "lib/connectService.js",
  "dependencies": {
    "getenv": "0.7.0",
    "@sealsystems/consul": "4.0.1",
    "@sealsystems/log": "1.3.1",
    "@sealsystems/tlscert": "2.1.1"
  },
  "devDependencies": {
    "@sealsystems/eslint-config-es": "1.1.1",
    "assertthat": "2.0.0",
    "nock": "10.0.2",
    "nodeenv": "1.0.0",
    "proxyquire": "2.1.0",
    "roboter": "0.16.0",
    "roboter-server": "0.16.0"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/sealsystems/node-connect-service.git"
  },
  "release": {
    "extends": "@sealsystems/semantic-release-config/node-module"
  },
  "publishConfig": {
    "access": "public"
  }
}
